package assie;
import java.util.Scanner;
public class oe {
	public static void main() {
		Scanner scr =new Scanner(System.in);
		System.out.println("give the number");
		int a= scr.nextInt();
		System.out.println("odd"+" "+" even");
		for(int i=1;i<=a;i++) {
			if(i%2!=0) {
				System.out.print(i);
			}
			else {
				System.out.println("      "+i);
			}
		}
		
		

	}

	}

