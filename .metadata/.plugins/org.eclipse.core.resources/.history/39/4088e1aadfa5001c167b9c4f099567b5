
public class hello {
	public static void main(String[] args)
	{
		int a=20;
		int b=30;
		int c=40;
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a/b);
		System.out.println(a*b);
		System.out.println(a%b);
		System.out.println("logic");
		System.out.println(a<b&& ++c>b);
		System.out.println(a++>b || ++c<b);
		System.out.println(a++>b || ++c<b++);
		
		System.out.println(a>=b);
		System.out.println(a<=b);
		System.out.println(a==b);
		System.out.println(a!=b);
		System.out.println(~c);
	
	
	}
}
