package assie;
import java.util.Scanner;
public class s1 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("give the 3 numbers:");
		int a= sc.nextInt();
		int b= sc.nextInt();
		int c= sc.nextInt();
		int sum=0;
		for(int i=0;i<c;i++) {
			for(int j=0;j<c;j++) {
				sum+=(a+(2^i)*b);
				System.out.println(sum);
			}
			
		}
	}
}
